{
    "manifest_version": 2,
    "name": "dateable",
    "description": "Making Scheduling Easier",
    "version": "1.0",

    "icons": {
        "128": "icon.png"
    },

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "background" :{
        "scripts": ["background.js"],
        "persistant": false
    },

    "permissions" : [
        "activeTab", 
        "contextMenus"
    ]
}
